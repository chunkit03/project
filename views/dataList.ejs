<html>
<head>
<title>View Restaurant</title>
</head>
<body>

<br>
<table border="1" cellpadding="5" cellspacing="5" align="center"">
<caption>Restaurant list</caption>
	<tr>
		<th>Name</th>
		<th>Street</th>
		<th>Zipcode</th>
		<th>Building</th>
		<th>Borough</th>
		<th>Cuisine</th>
		<th>GPS Coordinates (lon.)</th>
		<th>GPS Coordinates (lat.)</th>
		<th>Photo</th>
		<th>Reated</th>
	</tr>
	<% for (var i=0; i<rests.length; i++) { %>
	  <tr>
       <td><%= rests[i].name %></td>
       <td><%= rests[i].address.street %></td>
	   <td><%= rests[i].address.zipcode %></td>
	   <td><%= rests[i].address.building %></td>
	   <td><%= rests[i].borough %></td>
	   <td><%= rests[i].cuisine %></td>
	   <td><%= rests[i].lon %></td>
	   <td><%= rests[i].lat %></td>
	   <td><%= rests[i].image %></td>
	   <td><a href="/rated">Rate</a></td>
      </tr>
	<% } %>

</table>
<p style="text-align:center">
<p align="center">
<a href="/function">Back</a>
</p>
</body>
</html>
